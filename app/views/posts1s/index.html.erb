<p id="notice"><%= notice %></p>

<h1>Posts1s</h1>

<table>
  <thead>
    <tr>
      <th>Body</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @posts1s.each do |posts1| %>
      <tr>
        <td><%= posts1.body %></td>
        <td><%= posts1.user_id %></td>
        <td><%= link_to 'Show', posts1 %></td>
        <td><%= link_to 'Edit', edit_posts1_path(posts1) %></td>
        <td><%= link_to 'Destroy', posts1, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Posts1', new_posts1_path %>
